import lombok.Data;
import lombok.NoArgsConstructor;

@Data
@AllArgsConstructor
@NoArgsConstructor
@Component
public class OrdersDto {
	private int orderid;
	private String amount;
	private LocalDateTime odate;
    private User user;
}
